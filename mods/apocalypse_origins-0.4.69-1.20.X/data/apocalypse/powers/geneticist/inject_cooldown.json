{
	"type": "origins:multiple",
	"resource":
	{
		"type": "origins:resource",
		"min": 0,
		"max": 30,
		"start_value": 30,
		"hud_render": {
			"should_render": false,
			"bar_index": 6
		}
	},
	"timer":
	{
		"type": "origins:action_over_time",
		"interval": 20,
		"entity_action": {
			"type": "origins:if_else_list",
			"actions": [
				{
					"condition": {
						"type": "origins:resource",
						"comparison": ">",
						"resource": "*:*_resource",
						"compare_to": 0
					},
					"action": {
						"type": "origins:change_resource",
						"resource": "*:*_resource",
						"change": -1
					}
				},
				{
					"condition": {
						"type": "origins:resource",
						"comparison": "==",
						"resource": "*:*_resource",
						"compare_to": 0
					},
					"action": {
						"type": "origins:execute_command",
						"command": "power revoke @s apocalypse:geneticist/inject_cooldown geneticist"
					}
				}
			]
		}
	},
	"glow":
	{
		"type": "origins:self_glow",
		"use_teams": false,
		"red": 0.3,
		"blue": 0.5,
		"green": 0.3,
		"entity_condition":{
			"type": "origins:origin",
			"origin": "apocalypse:geneticist",
			"layer": "origins:origin"
		},
		"bientity_condition": {
			"type": "origins:can_see"
		}
	}
}