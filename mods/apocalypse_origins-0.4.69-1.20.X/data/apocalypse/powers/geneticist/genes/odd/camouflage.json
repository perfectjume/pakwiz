{
	"type": "origins:multiple",
	"callback":
	{
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command": "scale reset pehkui:visibility @s"
		}
	},
	"resource":
	{
		"type": "origins:resource",
		"min": 0,
		"max": 1,
		"start_value": 0,
		"hud_render": {
			"should_render": true,
			"bar_index": 7,
			"sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
			"condition": {
				"type": "origins:resource",
				"comparison": "==",
				"resource": "*:*_resource",
				"compare_to": 1
			}
		}
	},
	"update_resource":
	{
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:if_else_list",
			"actions": [
				{
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:moving"
							},
							{
								"type": "origins:resource",
								"comparison": "==",
								"resource": "*:*_resource",
								"compare_to": 0
							}
						]
					},
					"action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:change_resource",
								"resource": "*:*_resource",
								"change": 1
							},
							{
								"type": "origins:execute_command",
								"command": "scale reset pehkui:visibility @s"
							}
						]
					}
				},
				{
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:moving",
								"inverted": true
							},
							{
								"type": "origins:resource",
								"comparison": "==",
								"resource": "*:*_resource",
								"compare_to": 1
							}
						]
					},
					"action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:change_resource",
								"resource": "*:*_resource",
								"change": -1
							},
							{
								"type": "origins:execute_command",
								"command": "scale set pehkui:visibility 0 @s"
							}
						]
					}
				}
			]
		},
		"interval": 10
	},
	"invis":
	{
		"type": "origins:invisibility",
		"render_armor": false,
		"condition": {
			"type": "origins:resource",
			"comparison": "==",
			"resource": "*:*_resource",
			"compare_to": 0
		}
	}
}