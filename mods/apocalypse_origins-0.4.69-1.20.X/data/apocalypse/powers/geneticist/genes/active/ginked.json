{
	"type": "origins:multiple",
	"durationbar": 
	{
		"type": "origins:resource",
		"min": 0,
		"max": 3,
		"start_value": 3,
		"hud_render": {
			"should_render": false,
			"bar_index": 0
		}
	},
	"timer": 
	{
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:if_else_list",
			"actions": [
				{
					"condition": {
						"type": "origins:resource",
						"comparison": ">",
						"resource": "*:*_durationbar",
						"compare_to": 0
					},
					"action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:execute_command",
								"command": "particle glow_squid_ink ~ ~1 ~ 0 0 0 0 1"
							},
							{
								"type": "origins:change_resource",
								"resource": "*:*_durationbar",
								"change": -1
							}
						]
					}
				},
				{
					"condition": {
						"type": "origins:resource",
						"comparison": "==",
						"resource": "*:*_durationbar",
						"compare_to": 0
					},
					"action": {
						"type": "origins:execute_command",
						"command": "power revoke @s apocalypse:geneticist/genes/active/ginked geneticist"
					}
				}
			]
		},
		"interval": 20
	},
	"lessarmor":
	{
		"type": "origins:attribute",
		"modifier": {
			"attribute": "minecraft:generic.armor",
			"operation": "multiply_total",
			"value": -0.2
		}
	},
	"extradamage":
	{
		"type": "origins:modify_damage_taken",
		"modifier": {
			"operation": "addition",
			"value": 2
		}
	},
	"hexblade_compat":
	{
		"type": "origins:self_action_when_hit",
		"entity_action":{
			"type": "origins:execute_command",
			"command": "resource change @s apocalypse:hexblade/hexblade_resource_souls -2"
		},
		"cooldown": 10
	}
}