{
	"type": "origins:multiple",
	"resource":
	{
		"type": "origins:resource",
		"min": 0,
		"max": 600,
		"start_value": 600,
		"hud_render": {
			"should_render": false,
			"bar_index": 6
		}
	},
	"timer":
	{
		"type": "origins:action_over_time",
		"interval": 20,
		"entity_action": {
			"type": "origins:if_else_list",
			"actions": [
				{
					"condition": {
						"type": "origins:resource",
