					},
					"action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:execute_command",
								"command": "tellraw @s {\"text\":\"The effect of Antimutagens wore off...\",\"color\":\"aqua\"}"
							},
							{
								"type": "origins:execute_command",
								"command": "power revoke @s apocalypse:geneticist/antimutagens_buff geneticist"
							}
						]
					}
				}
			]
		}
	}
}