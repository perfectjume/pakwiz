{
	"type": "origins:multiple",
	"durationbar": 
	{
		"type": "origins:resource",
		"min": 0,
		"max": 10,
		"start_value": 10,
		"hud_render": {
			"should_render": false,
			"bar_index": 0
		}
	},
	"timer": 
	{
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:if_else_list",
			"actions": [
				{
					"condition": {
						"type": "origins:resource",
						"comparison": ">",
						"resource": "*:*_durationbar",
						"compare_to": 0
					},
					"action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:execute_command",
								"command": "particle glow_squid_ink ~ ~ ~ 0 0 0 0 3 normal @a[tag=!spore]"
							},
							{
								"type": "origins:execute_command",
								"command": "particle minecraft:electric_spark ~ ~ ~ 0 0.15 0 0 3 normal @a[tag=!spore]"
							},
							{
								"type": "origins:execute_command",
								"command": "effect give @s minecraft:glowing 2 0 true"
							},
							{
								"type": "origins:change_resource",
								"resource": "*:*_durationbar",
								"change": -1
							},
							{
								"type": "origins:execute_command",
								"command": "execute at @s run particle entity_effect ~ ~1 ~ 0 1 0 1 1 normal @a[tag=!spore]"
							}
						]
					}
				},
				{
					"condition": {
						"type": "origins:resource",
						"comparison": "==",
						"resource": "*:*_durationbar",
						"compare_to": 0
					},
					"action": {
						"type": "origins:execute_command",
						"command": "power revoke @s apocalypse:myconoids/obvious_position tempmyco"
					}
				}
			]
		},
		"interval": 20
	}
}