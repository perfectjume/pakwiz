	"use_nether_shard":
	{
		"type": "origins:action_on_block_use",
		"entity_action":{
			"type": "origins:if_else_list",
			"actions": [
				{
					"condition":{
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:resource",
								"comparison": "==",
								"resource": "*:*_blockusing",
								"compare_to": 0
							},
							{
								"type": "origins:dimension",
								"dimension": "minecraft:the_nether",
								"inverted": true
							}
						]
					},
					"action":{
						"type": "origins:and",
						"actions": [ 
							{
								"type": "origins:change_resource",
								"resource": "*:*_blockusing",
								"change": 1
							},
							{
								"type": "origins:if_else_list",
								"actions": [
									{
										"condition":{
											"type": "origins:resource",
											"comparison": "==",
											"resource": "apocalypse:refractor2/passives_tpmodea",
											"compare_to": 1
										},
										"action":{
											"type": "origins:execute_command",
											"command": "execute as @e[distance=..2] run execute in minecraft:the_nether run tp @s ~ ~ ~"
										}
									},
									{
										"condition":{
											"type": "origins:or",
											"conditions": [
												{
													"type": "origins:resource",
													"comparison": "==",
													"resource": "apocalypse:refractor2/passives_tpmodea",
													"compare_to": 0
												},
												{
													"type": "origins:origin",
													"origin": "apocalypse:refractor",
													"layer": "origins:origin",
													"inverted": true
												}
											]
										},
										"action":{
											"type": "origins:execute_command",
											"command": "execute in minecraft:the_nether run tp @s ~ ~ ~"
										}
									}
								]
							},
							{
								"type": "origins:execute_command",
								"command": "effect clear @s"
							},
							{
								"type": "origins:extinguish"
							},
							{
								"type": "origins:execute_command",
								"command": "playsound minecraft:block.beacon.power_select master @a ~ ~ ~ 1 2"
							},
							{
								"type": "origins:execute_command",
								"command": "summon area_effect_cloud ~ ~ ~ {Particle:\"minecraft:dripping_obsidian_tear\",Radius:2f,Duration:300,RadiusPerTick:-0.1f}"
							}
						]
					}
				}
			]
		},
